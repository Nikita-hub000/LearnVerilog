{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
      "jest": {
        "moduleFileExtensions": ["js", "json", "vue"],
        "transform": {
          "^.+\\.js$": "babel-jest",
          "^.+\\.vue$": "@vue/vue2-jest"
        }
      }
  }
}
